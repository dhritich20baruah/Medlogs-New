{
  "expo": {
    "name": "medlogs_new",
    "slug": "medlogs_new",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCalendarsUsageDescription": "Allow $(PRODUCT_NAME) to access your calendar to add medical reminders.",
        "NSRemindersUsageDescription": "Allow $(PRODUCT_NAME) to access your reminders to add medication alerts."
      }
    },
    "android": {
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dhritich20baruah.medlogs_new"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      [
         "react-native-google-mobile-ads",
      {
         "androidAppId": "ca-app-pub-4558946228793580~5580589882"
      }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "235a7770-7275-4f5d-8579-0f5805006a03"
      }
    }
  }
}
